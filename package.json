{
  "name": "ghpagesdemo",
  "version": "0.1.0",
  "private": true,
  "description": "My Electron app",
  "author": "maheshb@advaintai.com",
  "main": "electron/main.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron:build": "rimraf dist && react-scripts build && electron-builder",
    "test": "react-scripts test --watch=false",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.mahesh.ghpagesdemo",
    "productName": "MyElectronApp",
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
      "extraMetadata": {
      "main": "electron/main.js"
    },
    "directories": {
      "buildResources": "assets"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "MaheshAdvaint",
        "repo": "ghpagesdemo"
      }
    ],
    "win": {
      "target": "nsis"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "gh-pages": "^6.3.0",
    "rimraf": "^6.1.2",
    "wait-on": "^9.0.3"
  }
}
